# Uncomment this line to define a global platform for your project
platform :ios, '9.0'

target 'QuickFill' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for QuickFill

  pod 'Masonry'
  pod 'Fabric'
  pod 'Crashlytics'
  
  target 'QuickFillKeyboardTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'QuickFillKeyboardUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'QuickFillKeyboard' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for QuickFillKeyboard

  pod 'Masonry'
  pod 'Appirater'
  pod 'Fabric'
  pod 'Crashlytics'
  
end

